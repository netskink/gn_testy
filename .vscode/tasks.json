{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++:clang gn build active file",
			"command": "/opt/homebrew/bin/ninja",
			"args": [
				"-v",
				"-C",
				"out"
			],
			"options": {
				"cwd": "${fileDirname}/.."
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/clang++"
		}
	]
}